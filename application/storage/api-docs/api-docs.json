{
    "openapi": "3.0.0",
    "info": {
        "title": "API Agropecuária",
        "description": "Documentação dos endpoints da API Agropecuária",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Servidor Local"
        }
    ],
    "paths": {
        "/api/producers": {
            "get": {
                "tags": [
                    "Produtores"
                ],
                "summary": "Listar todos os produtores",
                "description": "Retorna uma lista de todos os produtores cadastrados.",
                "operationId": "getProducersList",
                "responses": {
                    "200": {
                        "description": "Lista retornada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "José da Silva"
                                            },
                                            "cpf_cnpj": {
                                                "type": "string",
                                                "example": "123.456.789-00"
                                            },
                                            "telefone": {
                                                "type": "string",
                                                "example": "(11) 99999-9999"
                                            },
                                            "email": {
                                                "type": "string",
                                                "example": "jose@email.com"
                                            },
                                            "endereco": {
                                                "type": "string",
                                                "example": "Rua das Laranjeiras, 123 - SP"
                                            },
                                            "data_cadastro": {
                                                "type": "string",
                                                "format": "date",
                                                "example": "2025-01-10"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/properties": {
            "get": {
                "tags": [
                    "Propriedades"
                ],
                "summary": "Listar todas as propriedades",
                "operationId": "ae50890d9da51a5cfe01cd2151d4077a",
                "parameters": [
                    {
                        "name": "producer_id",
                        "in": "query",
                        "description": "Filtra propriedades por produtor",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de propriedades retornada com sucesso"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Produtores",
            "description": "Produtores"
        },
        {
            "name": "Propriedades",
            "description": "Propriedades"
        }
    ]
}